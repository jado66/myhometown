{
  "functions": {
    "app/api/communications/send-texts/route.js": {
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "ENABLE_EXPERIMENTAL_COREPACK": "1"
    }
  },
  "crons": [
    {
      "path": "/api/cron/process-scheduled-texts",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/test-text",
      "schedule": "*/5 * * * *"
    }
  ]
}
